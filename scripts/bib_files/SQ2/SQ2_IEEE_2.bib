@INPROCEEDINGS{9835384,
  author={Lombardi, Johann},
  booktitle={2022 IEEE International Parallel and Distributed Processing Symposium Workshops (IPDPSW)}, 
  title={ESSA 2022 Invited Speaker DAOS: Nextgen Storage Stack for HPC and AI}, 

  abstract={To solve Particle Swarm Optimization (PSO) to plunge into the local extremums, this paper introduces parallel calculation and chaos operator and puts forward a parallel chaos particle swarm optimization. This method divides particle population into subpopulations for asynchronous and parallel chaos optimization. So it improves the search speed and keeps population diversity to avoid premature. The simulation results show this method is better than PSO.},
  keywords={Chaos;Particle swarm optimization;Optimization methods;Particle tracking;Application software;Computer science;Educational institutions;Reliability engineering;Industrial engineering;Evolutionary computation;particle swarm optimization;chaos;parallel calculation},
  doi={10.1109/ESIAT.2009.349},
  ISSN={},
  month={July},}@INPROCEEDINGS{9719870,
  author={Moon, Jeong-sun and Grabar, Bob and Wong, Joel and Dao, Chuong and Arkun, Erdem and Morales, Didiel V. and Tai, Joe and Fanning, Dave and Venkatesan, Nivedhita and Fay, Patrick},
  booktitle={2022 IEEE Topical Conference on RF/Microwave Power Amplifiers for Radio and Wireless Applications (PAWR)}, 
  title={Ultra-Linear and High-Efficiency GaN Technology for 5G and Beyond}, 
  year={2022},
  volume={},
  number={},
  pages={5-7},
  abstract={Realizing the full potential of mm-Wave 5G and beyond communication systems will require transistors with higher linearity and efficiency than current technology options. While GaN HEMTs offer excellent power density, their limited linearity and efficiency, especially under complex modulations, compromises overall system performance and requires substantial digital pre-distortion compensation as well as substantial power back-off. Here, we report the large-signal, two-tone linearity of graded-channel GaN HEMTs at 30 GHz, and compare their performance to that of conventional AlGaN/GaN HEMTs. In class AB bias, a two-tone P AE of 62% was obtained experimentally, which is a state-of-the-art result. We find that ~3 dB back-off from peak P AE is sufficient to achieve a carrier to third-order intermodulation (C/IM3) ratio of 30 dBc. Further, the efficiency under this back-off condition provides an associated P AE of 55% at 30 GHz. Hence, the GaN HEMTs are promising for mm-wave linear and efficient amplifiers, including 5G applications.},
  keywords={Wireless communication;Power system measurements;5G mobile communication;System performance;Linearity;Power amplifiers;HEMTs;GaN;amplifiers;communications;5G;6G;PAE;linearity;Ka-band},
  doi={10.1109/PAWR53092.2022.9719870},

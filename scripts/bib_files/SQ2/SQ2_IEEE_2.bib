  title={High efficiency small antenna for WLAN application}, 
  year={2013},
  volume={},
  number={},
  pages={757-760},
  abstract={In this paper, a small metamaterial antenna with high efficiency is presented. The antenna is proposed for 2.4 GHz Wireless Local Area Network (WLAN) application. The metamaterial resonant antenna is designed with a mushroom structure which based on the zeroth order resonant characteristic of a composite right/left-handed transmission line (CRLH TL). The antenna is designed on the low-cost FR4 substrate with dielectric constant εr = 4.4 and tan δ= 0.02. The antenna is simulated and optimized by HFSS 14. The simulation results show that antenna has small size and a good bandwidth with range from 2.40-2.48 GHz for return loss S11 less than -10 dB. The proposed antenna offers high efficiency 72-75% in operation bandwidth. The overall dimension of the antenna is 20.0×22.0×1.6 mm3 including the antenna substrate.},
  keywords={Resonant frequency;Metamaterials;Antenna radiation patterns;Wireless LAN;Broadband antennas;Substrates},
  doi={10.1109/APWC.2013.6624903},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{9301651,

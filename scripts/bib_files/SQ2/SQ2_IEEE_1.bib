  author={Povyshev, Aleksandr A. and Sokolov, Alexander N.},
  booktitle={2024 International Russian Smart Industry Conference (SmartIndustryCon)}, 
  title={Ensuring the Integrity and Availability of Information in the Model of Decentralized Data Storage System}, 
  year={2024},
  volume={},
  number={},
  pages={517-521},
  abstract={A model of decentralized data storage system is presented to ensure the integrity and availability of information based on decentralized management algorithm. The proposed decentralized management algorithm solves the problem of a rapidly growing IP address table due to the increasing number of system users. We propose an approach focused on optimizing the direct links between the hosts of the storage network and dynamically calculating their ratings. Host ratings are calculated based on network latency, uptime, and processor performance. The rating calculation is a two-stage process, the second stage takes CPU performance into account. This model offers a tiered structure of masterhosts, where first-level masterhosts accumulate host IP addresses and provide access to their database, maintaining the current state of the decentralized storage network, second-level masterhosts aggregate the IP addresses of first-level masterhosts into groups, thus creating a hierarchical network structure. The paper evaluates the proposed approach by calculating the consumed service traffic required to maintain each host, and compares the decentralized storage scheme and the centralized scheme. The results show that the decentralized system is superior to the centralized one in terms of data availability and has a higher efficiency of disk space utilization due to savings on data duplication, which proves the possibility of effective existence of such a system, and thus its application to ensure the integrity and availability of stored data.},
  keywords={Buffer storage;Heuristic algorithms;Scalability;Encoding;Data models;Central Processing Unit;Encryption;decentralization;availability;redundant coding;information redundancy;data storage;information security threats;masterhost},
  doi={10.1109/SmartIndustryCon61328.2024.10515948},
  ISSN={},
  month={March},}@INPROCEEDINGS{10039398,
  author={MIYAZAKI, Ryo and YASUTA, Yuto and HAN, Xiao and TOMITA, Kohji and KAMIMURA, Akiya},
  booktitle={2023 IEEE/SICE International Symposium on System Integration (SII)}, 
  title={Decentralized Multi-UAV Formation Control and Navigation over a Self-Organizing Coordination Network}, 
  year={2023},
  volume={},
  number={},
  pages={1-6},
  abstract={Autonomous control technology for multiple network-connected multi-rotors and simultaneous remote control by few operators has applications in disaster investigation. Challenges in constructing such a system include path-planning for multiple multi-rotors. Accurate position estimation methods such as GNSS already exist for outdoor self-positioning; however, no optimal method has been established for the path-planning problem involving multi-rotors in dynamic and complex environments. Herein, in addition to the conventional artificial potential field method, we propose an extended function that introduces a rotational force field, which enables autonomous navigation from a starting point to the target while avoiding obstacles and local minima. Additionally, we realized autonomous decentralized formation control and navigation with multiple multi-rotors. This was achieved by applying a virtual spring-damper model between the multi-rotors based on the relative distance information calculated from global positions obtained by inter multi-rotor communication. To confirm the practicality of our proposed method, we generated a two-dimensional map from the three-dimensional model data of a real urban area and conducted simulations for various multiple multi-rotor applications, successfully demonstrating formation control and navigation in complex environments.},
  keywords={Solid modeling;Global navigation satellite system;Navigation;Urban areas;Force;Estimation;System integration},
  doi={10.1109/SII55687.2023.10039398},
  ISSN={2474-2325},
  month={Jan},}@INPROCEEDINGS{10338926,

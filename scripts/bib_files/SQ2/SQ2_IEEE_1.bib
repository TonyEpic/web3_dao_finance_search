  ISSN={},
  month={March},}@INPROCEEDINGS{10449575,
  author={Chandgude, Nishant Vikas and Salunke, Aniket Sunil and Bawankar, Chetan and Kumar, Vishant},
  booktitle={2023 4th International Conference on Computation, Automation and Knowledge Management (ICCAKM)}, 
  title={Transparent and Tamper-Proof Certificate Verification on the Blockchain}, 
  year={2023},
  volume={},
  number={},
  pages={1-6},
  abstract={In the realm of certificate generation and verification, traditional systems have long faced challenges associated with trust, security, and transparency. This paper presents a pioneering solution that leverages blockchain technology to address these inherent issues. Our proposal introduces a decentralized application (DApp) built on the Polygon blockchain, specifically designed for the generation and verification of certificates. By doing so, we establish an unassailable foundation for transparent, tamper-proof, and secure certificate management. This novel approach ensures the integrity of certificates and bolsters confidence in their authenticity. The practical implementation of our DApp, combined with the immutability of blockchain, not only overcomes the limitations of conventional systems but also paves the way for a trustworthy and efficient certification ecosystem.},
  keywords={Automation;Ecosystems;Decentralized applications;Knowledge management;Blockchains;Security;Proposals;Blockchain;smart contract;certificates;Polygon;DApp},

@inproceedings{10.1145/3628354.3629531,
author = {Gigli, Lorenzo and Montori, Federico and Galletti, Giacomo and Sciullo, Luca and Bedogni, Luca},
title = {Design and Development of a Mobile Dapp for Mobile Crowdsensing over EVM-enabled Blockchains},
year = {2024},
isbn = {9798400704390},
publisher = {Association for Computing Machinery},
